{
  "name": "@banegasn/svelte-components",
  "version": "3.0.0",
  "description": "Svelte components library",
  "type": "module",
  "svelte": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "svelte": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./SvelteButton.svelte": {
      "types": "./dist/SvelteButton.svelte.d.ts",
      "svelte": "./dist/SvelteButton.svelte",
      "default": "./dist/SvelteButton.svelte"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "svelte-package -i src",
    "dev": "svelte-package -i src --watch",
    "clean": "rm -rf dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/banegasn/components.git",
    "directory": "packages/svelte-components"
  },
  "keywords": [
    "svelte",
    "components",
    "ui"
  ],
  "author": "banegasn",
  "license": "MIT",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com",
    "access": "public"
  },
  "dependencies": {
    "svelte": "^4.2.19"
  },
  "devDependencies": {
    "@sveltejs/package": "^2.3.5",
    "@sveltejs/vite-plugin-svelte": "^3.1.2",
    "svelte-check": "^4.0.4",
    "typescript": "^5.6.3",
    "vite": "^5.4.9"
  },
  "peerDependencies": {
    "svelte": "^4.0.0"
  }
}
